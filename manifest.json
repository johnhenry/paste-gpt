{
  "manifest_version": 3,
  "version": "0.0",
  "name": "Paste GPT",
  "description": "An extension to paste web content directly into Chat GPT",
  "icons": {
    "16": "images/icons/logo_16.png",
    "32": "images/icons/logo_32.png",
    "48": "images/icons/logo_48.png",
    "64": "images/icons/logo_64.png",
    "128": "images/icons/logo_128.png"
  },
  "action": {
    "default_popup": "popup/index.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "https://chatgpt.com/*"
  ]
}